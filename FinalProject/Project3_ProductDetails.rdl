  <DataSets>
    <DataSet Name="dtsProductDetails">
      <Query>
        <DataSourceName>dtscSharedContosoDW</DataSourceName>
        <CommandText>SELECT   DimProduct.ProductName, DimProduct.ProductDescription, DimProductCategory.ProductCategoryName, DimProductSubcategory.ProductSubcategoryName, 
                         DimProduct.Manufacturer, DimProduct.UnitPrice, DimProduct.ProductKey
FROM         DimProduct INNER JOIN
                         DimProductSubcategory ON DimProduct.ProductSubcategoryKey = DimProductSubcategory.ProductSubcategoryKey INNER JOIN
                         DimProductCategory ON DimProductSubcategory.ProductCategoryKey = DimProductCategory.ProductCategoryKey</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductDescription">
          <DataField>ProductDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
