  <DataSets>
    <DataSet Name="dtsNetRevenue">
      <Query>
        <DataSourceName>dtscSharedContosoDW</DataSourceName>
        <CommandText>SELECT   SUM(S.SalesAmount) - SUM(S.ReturnAmount) - SUM(S.DiscountAmount) AS 'Net Revenue', SUM(S.SalesQuantity) - SUM(S.ReturnQuantity) AS 'Net Sold Quantity', 
                         D.CalendarYear, P.ProductName, S.ProductKey AS ProductKey
FROM         FactSales AS S INNER JOIN
                         DimDate AS D ON S.DateKey = D.Datekey INNER JOIN
                         DimProduct AS P ON S.ProductKey = P.ProductKey
GROUP BY D.CalendarYear, P.ProductName, S.ProductKey</CommandText>
      </Query>
      <Fields>
        <Field Name="Net_Revenue">
          <DataField>Net Revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Net_Sold_Quantity">
          <DataField>Net Sold Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductKey">
          <DataField>ProductKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!CalendarYear.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!prmYear.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="dtsYear">
      <Query>
        <DataSourceName>dtscSharedContosoDW</DataSourceName>
        <CommandText>SELECT DISTINCT DimDate.CalendarYear
FROM     DimDate INNER JOIN
                  FactSales ON DimDate.Datekey = FactSales.DateKey</CommandText>
      </Query>
      <Fields>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
