  <DataSets>
    <DataSet Name="dtsSalesByYearAndChannel">
      <Query>
        <DataSourceName>sharedDtscContosoDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prmYear">
            <Value>=Parameters!prmYear.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SUM(FactSales.SalesAmount) AS SalesAmount, DimDate.CalendarYear, DimChannel.ChannelName
FROM     FactSales INNER JOIN
                  DimDate ON FactSales.DateKey = DimDate.Datekey INNER JOIN
                  DimChannel ON FactSales.channelKey = DimChannel.ChannelKey
GROUP BY DimDate.CalendarYear, DimChannel.ChannelName</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ChannelName">
          <DataField>ChannelName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!CalendarYear.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!prmYear.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="dtsMostRecentYearSales">
      <Query>
        <DataSourceName>sharedDtscContosoDW</DataSourceName>
        <CommandText>SELECT SUM(FactSales.SalesAmount) AS SalesAmount, DimDate.CalendarYear, DimChannel.ChannelName
FROM     FactSales INNER JOIN
                  DimDate ON FactSales.DateKey = DimDate.Datekey INNER JOIN
                  DimChannel ON FactSales.channelKey = DimChannel.ChannelKey
WHERE YEAR(FactSales.DateKey) = (SELECT YEAR(MAX(FactSales.DateKey)) FROM FactSales)
GROUP BY DimDate.CalendarYear, DimChannel.ChannelName</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ChannelName">
          <DataField>ChannelName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
